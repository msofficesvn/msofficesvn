<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="Subversion" label="Subversion">
				<group id="SVNGroup" label="Subversion">
					<button id="Update" label="Mettre à jour" imageMso="FileCheckOut" size="large"  onAction="UpdateOnAct" />
					<button id="Lock" label="Verrouiller" imageMso="Lock" size="large" onAction="LockOnAct" />
					<button id="Commit" label="Livrer" imageMso="FileCheckIn" size="large" onAction="CommitOnAct" />
					<button id="Diff" label="Comparer" imageMso="ReviewCompareTwoVersions" size="large"  onAction="DiffOnAct" />
					<button id="Log" label="Journal" imageMso="ReviewTrackChanges" size="large"  onAction="LogOnAct" />
					<button id="RepoBrowser" label="Explorateur de dépôt" imageMso="LookUp" size="large"  onAction="RepoBrowserOnAct" />
					<button id="UnLock" label="Déverrouiller" imageMso="AdpPrimaryKey" onAction="UnLockOnAct" />
					<button id="Add" label="Ajouter" imageMso="OutlineExpand" onAction="AddOnAct" />
					<button id="Delete" label="Supprimer" imageMso="Delete" onAction="DeleteOnAct" />
				</group>
				<group id="File" label="File">
					<button id="Explorer" label="Explorateur" imageMso="FileOpen" size="large"  onAction="OpenExplorerOnAct" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
